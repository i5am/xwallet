{
  "name": "tether-wdk-wallet",
  "version": "1.0.0",
  "type": "module",
  "description": "Tether WDK Multi-chain Wallet (Hot/Cold/Watch-only)",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "android:build": "npm run build && npx cap sync android",
    "android:open": "npx cap open android",
    "android:sync": "npx cap sync android",
    "ios:build": "npm run build && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "ios:sync": "npx cap sync ios",
    "ios:setup-github": "powershell -ExecutionPolicy Bypass -File ./scripts/setup-github-actions-ios.ps1",
    "ios:setup-eas": "powershell -ExecutionPolicy Bypass -File ./scripts/setup-eas-build-ios.ps1",
    "eas:build:preview": "eas build --platform ios --profile preview",
    "eas:build:production": "eas build --platform ios --profile production"
  },
  "dependencies": {
    "@bitcoinerlab/secp256k1": "^1.2.0",
    "@capacitor/android": "^7.4.3",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.4",
    "@noble/hashes": "^2.0.1",
    "@noble/secp256k1": "^1.7.1",
    "axios": "^1.6.2",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "buffer": "^6.0.3",
    "clsx": "^2.0.0",
    "core-js": "^3.46.0",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "ecpair": "^3.0.0",
    "ethers": "^6.9.0",
    "events": "^3.3.0",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.294.0",
    "process": "^0.11.10",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "regenerator-runtime": "^0.14.1",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/crypto-js": "^4.2.1",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
