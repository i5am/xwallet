{
  "expo": {
    "name": "Tether WDK Wallet",
    "slug": "tether-wdk-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.tether.wdk.wallet",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "需要使用相机扫描二维码",
        "NSPhotoLibraryUsageDescription": "需要访问相册选择二维码图片",
        "NSFaceIDUsageDescription": "使用 Face ID 保护您的钱包"
      }
    },
    "android": {
      "package": "com.tether.wdk.wallet",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "78690e56-1b30-4a83-b271-2ab12376db55"
      }
    }
  }
}
