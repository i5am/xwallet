# 环境配置示例

# 这是一个示例环境变量文件
# 复制此文件为 .env.local 并填写实际的值

# API Keys（可选，用于更好的性能和速率限制）
# VITE_INFURA_API_KEY=your_infura_api_key_here
# VITE_ALCHEMY_API_KEY=your_alchemy_api_key_here
# VITE_ETHERSCAN_API_KEY=your_etherscan_api_key_here

# 网络配置（默认使用公共节点）
# VITE_BTC_RPC_URL=https://blockstream.info/api
# VITE_ETH_RPC_URL=https://cloudflare-eth.com

# 开发模式
VITE_DEV_MODE=true

# 默认网络（mainnet 或 testnet）
VITE_DEFAULT_NETWORK=testnet

# 启用调试日志
VITE_DEBUG=true
